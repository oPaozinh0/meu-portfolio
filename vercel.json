{
    "version": 2,
    "builds": [
      {
        "src": "/public/build/assets/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/build/assets/(.*)",
        "dest": "/public/build/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }